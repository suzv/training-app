<?xml version="1.0"?>
<!-- ==============================================================================
   Messaging Configuration
  ============================================================================== -->
<messaging-config
  xmlns="http://guidewire.com/messaging-config"
  defaultServer="#messaging">
  <messaging>
    <destination
      id="65"
      initialretryinterval="500"
      internal="true"
      maxretries="5"
      name="Java.MessageDestination.Email.Name"
      transportplugin="emailMessageTransport"
      multithreadnonsafeorderedmessages="false"
      singlethreadandblockonnonsafeorderedmessages="false"/>
    <destination
      id="70"
      internal="true"
      name="Java.MessageDestination.PolicySystemTransport.Name"
      transportplugin="PCPolicySystemTransport">
      <event
        name="ABContactChanged"/>
      <event
        name="ABContactRemoved"/>
      <event
        name="ABContactPendingChangeRejected"/>
      <event
        name="ABContactCreatedApproved"/>
    </destination>
    <destination
      id="71"
      internal="true"
      name="Java.MessageDestination.ClaimSystemTransport.Name"
      transportplugin="CCClaimSystemTransport">
      <event
        name="ABContactChanged"/>
      <event
        name="ABContactRemoved"/>
      <event
        name="ABContactPendingChangeRejected"/>
      <event
        name="ABContactCreatedApproved"/>
    </destination>
    <destination
      id="72"
      internal="true"
      name="Java.MessageDestination.BillingSystemTransport.Name"
      transportplugin="BCBillingSystemTransport">
      <event
        name="ABContactChanged"/>
      <event
        name="ABContactRemoved"/>
      <event
        name="ABContactPendingChangeRejected"/>
      <event
        name="ABContactCreatedApproved"/>
    </destination>
    <destination
      disabled="false"
      id="324"
      internal="true"
      name="Java.MessageDestination.DocumentStore.Name"
      transportplugin="documentStoreTransport"
      multithreadnonsafeorderedmessages="false"
      singlethreadandblockonnonsafeorderedmessages="false">
      <event
        name="DocumentStore"/>
      <event
        name="FailedDocumentStore"/>
    </destination>
  </messaging>
</messaging-config>