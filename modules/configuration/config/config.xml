-->
  <!-- If false, the destruction of personal data will be disabled. -->
  <param name="PersonalDataDestructionEnabled" value="false"/>

  <!-- Minimum number of days that must pass after ContactPurgeRequest has been executed for the request to be eligible for purging. -->
  <param name="ContactDestructionRequestAgeForPurgingResults" value="10" />

  <!--
  ==============================================================================
    WS-I Webservices
  ==============================================================================
  -->

  <!-- the path to a list of WS-I webservices that are available to the WS-I webservices servlet.  This is
   first checked against config, then against Gosu type system, then against class loader, and finally
   as a file path (or fully qualified).

   If empty then it will search the typesystem for any @WsiWebService
   Shared with CC
  -->
  <param name="WebservicesAvailableServices" value=""/>

  <!-- "true" if the WS-I service listing page is to be displayed, false otherwise.
       Shared with CC
  -->
  <param name="WebservicesHideListPage" value="false"/>

  <!--  Whether the RequestVar objects are available from @WsiWebservice and gosu @Servlet originating threads.
  -->
  <param name="ScopedVariableSupportedForAllCapturedRequests" value="false"/>

  <!-- Enable this to update to 64bit IDs in the database and on SQLServer it will also update to datetime2 datatypes for timestamps -->
  <param name="MigrateToLargeIDsAndDatetime2" value="true"/>

  <!-- (SQL Server) Whether to provide comments with contextual information in certain SQL Select statements sent to the relational database.
       The comments are generated by instrumentation in higher level database objects created by using the query builder APIs.
  -->
  <param name="IdentifyQueryBuilderViaComments" value="true"/>
  <!-- (SQL Server) Whether to provide comments with contextual information in certain SQL Select statements sent to the relational database.
       The comments are generated by instrumentation in lower level objects, such as beans, typelists, and other database building blocks.
  -->
  <param name="IdentifyORMLayerViaComments" value="false"/>

  <!-- Number of threads to use when running the consistency checker. -->
  <param name="ConsistencyCheckerThreads" value="5"/>

  <!-- The time between refreshes of the zone cache, in seconds -->
  <param name="ZoneCacheRefreshIntervalSecs" value="86400"/>

  <!-- The latest date allowed to be used. -->
  <param name="MaxAllowedDate" value="2200-12-31"/>
  <!-- The earliest date allowed to be used. -->
  <param name="MinAllowedDate" value="1800-01-01"/>

  <!-- OSGi plugins startup timeout -->
  <param name="PluginStartupTimeout" value="60"/>

  <!-- The keyboard shortcut to activate the QuickJump box -->
  <param name="QuickJumpShortcut" value="/"/>

  <!-- Old external TransactionIds are purged when then have been there for this number of days -->
  <param name="TransactionIdPurgeDaysOld" value="30"/>

  <!-- The number of days to keep profiler data before they can be deleted. -->
  <param name="ProfilerDataPurgeDaysOld" value="30"/>


  <!--
    Enable/Disable an external Configuration Service as a source for some of application configuration.
    If enabled the application will load and manage configuration through a Configuration Service.
    When disabled existing local configuration is used.
  -->
  <param name="ConfigurationServiceEnabled" value="false"/>

  <!--
  The maximum allowable XML export size (in megabytes) which may be loaded into memory.
  Any attempt to export via admin API larger amount of data than this will fail.
  This parameter protects the server from possible memory consumption problems.
  -->
  <param name="MaximumXmlExportSize" value="300"/>

