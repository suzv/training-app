<?xml version="1.0"?>

<config xmlns="http://guidewire.com/config">

  <registry roles="batch, workqueue, scheduler, messaging, startable, ui" />

  <!--
  ==============================================================================
    ContactManager Parameters
  ==============================================================================
  -->

  <!-- The maximum number of search results before the UI warning appears.
       Shared with CC
  -->
  <param name="MaxContactSearchResults" value="300"/>

  <!-- The max number of duplicate contacts to be registered as duplicates by
    com.guidewire.ab.domain.contact.DuplicateContactsFinderWorkQueue.
    If more than this max number of duplicates are found for a given contact,
    DuplicateContactsFinderWorkQueue will not register any duplicates for that contact,
    but instead will log the contact's LinkID.
  -->
  <param name="MaxDuplicateContactsFinderWorkQueueResults" value="1000"/>

  <!--
     Indicates whether the search for duplicate contacts should widen and not use subtype specific logic.
     This parameter applies only to searches for duplicate contacts performed by the find duplicates batch
     process. It does not set general searching behavior in FindDuplicatesPlugin.  Defaults to true.
  -->
  <param name="DuplicateContactsWideSearch" value="true"/>

  <!--
    This value controls the maximum number of specialist services that can be specified in a search for
    contacts in CM. The value is limited by the database configuration being used, if too many are being
    used the database can run out of resources processing the IN clauses in the query.
  -->
  <param name="MaxNumberServicesInSearchQuery" value="20"/>

  <!--
    The duplicate contacts batch process acts on contacts created or modified after the last
    time it was run AND after the date specified by this parameter.  This makes it possible to avoid processing
    every contact in the database the first time the batch process run.  For example, if contacts are imported
    into the database on 8/21/2011 and this parameter is set to "08/22/2011 12:00 AM", those contacts will
    not be processed by the batch process unless they're later modified.
    Expected format: mm/dd/yyyy hh:mm am
  -->
  <!-- This is an optional parameter. When present, must contain valid value.
    <param name="DuplicateContactsEarliestModificationDate" value=""/>
  -->

  <!--
    The PendingChanges screen for updates now uses a cache to store the DiffDisplay entities related to the pending changes.
    These DiffDisplays contain the data used to display the differences being applied to a Contact in ContactManager based
    on a pending update from an external system. New PreUpdate rules are in place to evict DiffDisplays from the cache on
    any update to an ABContact or PendingContactChange entity.
  -->
  <!--
    The maximum number of DiffDisplay entities stored in the cache to increase performance in the Pending Changes
    screen for updates.
  -->
  <param name="MaxDiffDisplaysInCache" value="60"/>

  <!--
    The amount of time, in minutes, since last access that a DiffDisplay entity should be timed out of the cache
    for the Pending Changes screen for updates.
  -->
  <param name="DiffDisplaysCacheTimeoutInMinutes" value="10"/>

  <!-- The name of the method in "gw.api.data.DataSets" for generating AB sample data -->
  <param name="GosuSampleDataMethod" value="createDefaultSampleData"/>

  <!-- Whether to use the old-style pre-update framework or only to use the new Gosu class framework.
      If true, the  PreUpdate plugin class will be called and then the validation graph will be used to determine what beans should have their pre-update rules run.
      If false, only the PreUpdate plugin class will be invoked -->
  <param name="UseOldStylePreUpdate" value="true"/>

  <!-- Whether the production server retains debugging information. This parameter facilitates debugging from Studio without a type system refresh.
        - If set to true, ContactManager does not clear debug information after compilation.
        - If set to false, the server does not retain sufficient debugging information to enable debugging.
        As a production server does not recompile types, it is not possible to regenerate any debugging information. -->
  <param name="RetainDebugInfo" value="false"/>

  <!-- The default encryption id to use for all exports -->
  <param name="DefaultXmlExportIEncryptionId" value=""/>


  <!-- Velocity parameters; can be overridden -->
  <!-- NOTE velocimacro.library.autoreload MUST be set false in production -->
  <!-- NOTE file.resource.loader.modificationCheckInterval SHOULD be set to -1 in production (disables reloading the files) -->
  <!-- NOTE resource.manager.defaultcache.size SHOULD be a prime number larger than total number of templates -->
  <velocityParam name="file.resource.loader.cache" value="true"/>
  <velocityParam name="file.resource.loader.modificationCheckInterval"
                 value="-1"/>
  <velocityParam name="velocimacro.library" value="cc_VelocityMacros.vm"/>
  <velocityParam name="velocimacro.permissions.allow.inline" value="true"/>
  <velocityParam name="velocimacro.library.autoreload" value="false"/>
  <velocityParam name="velocimacro.permissions.allow.inline.to.replace.global"
                 value="true"/>
  <velocityParam name="resource.manager.defaultcache.size" value="127"/>

  <!--
  ==============================================================================
    UI Parameters
  ==============================================================================
  -->

  <!--  Shortcut for Action navigation menu -->
  <param name="ActionsShortcut" value="A"/>

  <!--  Shortcut for Uplink button -->
  <param name="UplinkShortcut" value="Up"/>

  <!-- The maximum number of autocomplete suggestions to show -->
  <param name="AutocompleteLimit" value="10"/>

  <!-- The Look & Feel theme for ClaimCenter -->
  <param name="UISkin" value="theme-9"/>

  <!-- If true, the help text for an input will be displayed when the input gets the focus. -->
  <param name="InputHelpTextOnFocus" value="true"/>

  <!-- If true, the help text for an input will be displayed when the user moves mouse over the input. -->
  <param name="InputHelpTextOnMouseOver" value="true"/>

  <!-- The time between refreshes of the script parameter info cache, in seconds -->
  <param name="ScriptParametersRefreshIntervalSecs" value="600"/>

  <!-- Time in seconds that a treeview state will be cached. -->
  <param name="TreeViewRefresh" value="120"/>

  <!-- The threshold year value for determining whether to resolve a two-digit year to the earlier or later century -->
  <param name="TwoDigitYearThreshold" value="50"/>

  <!-- The location of the web resources directory under the root of the tomcat config directory -->
  <param name="WebResourcesDir" value="resources"/>

  <!-- Whether or not the Gosu compiler generates errors if it detects unreachable code or missing return statements. -->
  <param name="UnreachableCodeDetection" value="true"/>

  <!-- Whether or not the Gosu compiler generates warnings if an implicit coercion is occurring in a program. -->
  <param name="WarnOnImplicitCoercion" value="true"/>


  <!--
  ==============================================================================
    Clustering Parameters
  ==============================================================================
  -->

  <!-- Is clustering enabled?
       Shared with CC
  -->
  <param name="ClusteringEnabled" value="false"/>

  <!--
    Cluster member database record update interval (in seconds). The same interval is used to reload list of cluster
    members from the database.
    Shared with CC
  -->
  <param name="ClusterMemberRecordUpdateIntervalSecs" value="60"/>

  <!--
    Number of minutes between each cluster statistics monitor logging (0 means disable statistics logging).
    Shared with CC
  -->
  <param name="ClusterStatisticsMonitorIntervalMins" value="60"/>

  <!-- The number of days to keep cluster member records before they can be deleted. -->
  <param name="ClusterMemberPurgeDaysOld" value="30"/>

  <!-- The time between refreshes of the group cache, in seconds -->
  <param name="GroupCacheRefreshIntervalSecs" value="600"/>

  <!-- If clustering is enabled, whether to check this server's configuration against the other
          servers in the cluster. -->
  <param name="ConfigVerificationEnabled" value="true"/>

  <!-- Whether a class name must be listed in the serialization whitelist to allow deserialization
       of its respective instances. -->
  <param name="SerializationWhitelistEnabled" value="false"/>

  <!-- The Content-Disposition setting to use when generated content (exports, printing, etc, but _not_ documents) is returned directly to the browser. Must be either 'inline' or 'attachment' (the default) -->
  <param name="DefaultContentDispositionMode" value="attachment"/>


  <!-- Time period (in minutes) when cached items are considered "active",
 i.e. the cache should give higher priority to preserve these items.
 This can be thought of as the period when items are being heavily used,
 e.g. how long a user stays on a page. It should be less than the reaping time.-->
  <param name="GlobalCacheActiveTimeMinutes" value="5"/>

  <!-- Time (in minutes) since last use before cached items are considered eligible for reaping. This can be thought of as the period during which
an entity is most likely to be re-used. -->
  <param name="GlobalCacheReapingTimeMinutes" value="15"/>

  <!-- The percentage of the heap to allocate to the global cache. -->
  <param name="GlobalCacheSizePercent" value="15"/>

  <!-- The amount of space to allocate to the global cache. If this integer value is specified, it will take precedence over GlobalCacheSizePercent.
  <param name="GlobalCacheSizeMegabytes" value=""/>
  -->

  <!-- Time (in minutes) since last write before cached items are considered stale and eligible for reaping. -->
  <param name="GlobalCacheStaleTimeMinutes" value="60"/>

  <!-- This parameter denotes the period of time (in days), in addition to today, for how long statistics are preserved for historical comparison. -->
  <param name="GlobalCacheStatsRetentionPeriodDays" value="7"/>

  <!-- This parameter denotes a period of time (in minutes) and is used for two purposes: 1) how long to preserve the reason why an item was       evicted, after the event occurred. When a cache miss occurs, the reason is looked up and reported in the CacheInfo page. 2) The period for        which to display statistics on the chart on the CacheInfo page.-->
  <param name="GlobalCacheStatsWindowMinutes" value="30"/>

  <!-- This parameter determines whether or not detailed statistics should be collected. -->
  <param name="GlobalCacheDetailedStats" value="false"/>

  <!--
  ==============================================================================
   Environment Parameters
  ==============================================================================
  -->

  <!-- denotes the username of the unrestricted user.  If not specified, defaults to "su"
       Shared with CC
  -->
  <param name="UnrestrictedUserName" value="su"/>

  <!-- Indicates if resoruces are mutable on this server. Studio uses this to
       determine whether or not to enable modification of resources.
       Shared with CC
  -->
  <param name="ResourcesMutable" value="true"/>

  <!-- Indicates whether beans can be put in a FrozenSet for inter-cluster transfer. -->
  <param name="FrozenSetsEnabled" value="false"/>

  <!--
    Cached range size key generator uses when generating individual keys. This
    cannot be a normal parameter in the database because we need it before the
    database has been initialized (and possibly upgraded)
    Shared with CC
  -->
  <param name="KeyGeneratorRangeSize" value="100"/>


  <!-- The default application locale if locale can't be determined from user or browser
       Shared with CC
  -->
  <param name="DefaultApplicationLocale" value="en_US"/>

  <!-- The default application language if language can't be determined from user or browser
       Shared with CC
  -->
  <param name="DefaultApplicationLanguage" value="en_US"/>

  <!-- The default country. This is also used as the default for new
       addresses that are created. This is a Country typecode, which normally
       correspond to the standard ISO two-letter codes.
       Shared with CC
  -->
