  <param name="FinalDocumentsNotEditable" value="false"/>

  <!--
  ==============================================================================
    Mime Type Information

    This section tells the system how to map between mime types, file
    extensions, and icons in the UI.

    Descriptions of these mimetypes can be found in the displaykey files.
    The descriptions have a prefix of "Mimetype." followed by the mimetype with "_" replacing all non-alphanumerics.

    For example, the "application/octet-stream" mimetype uses the "Mimetype.application_octet_stream" displaykey.
  ==============================================================================
  -->
  <mimetypemapping>
    <mimetype name="application/csv" extensions=".csv" icon="xls_mime"/>
    <mimetype name="application/msword" extensions=".doc" icon="word_mime"/>
    <mimetype name="application/octet-stream" extensions="" icon="document"/>
    <mimetype name="application/pdf" extensions=".pdf" icon="pdf_mime"/>
    <mimetype name="application/postscript" extensions=".ps" icon="pdf_mime"/>
    <mimetype name="application/rtf" extensions=".rtf" icon="text_mime"/>
    <mimetype name="application/vnd.ms-excel" extensions=".xls" icon="xls_mime"/>
    <mimetype name="application/vnd.ms-powerpoint" extensions=".ppt|.pps" icon="ppt_mime"/>
    <mimetype name="application/vnd.openxmlformats-officedocument.presentationml.presentation"
              extensions=".pptx" icon="ppt_mime"/>
    <mimetype name="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
              extensions=".xlsx" icon="xls_mime"/>
    <mimetype name="application/vnd.openxmlformats-officedocument.wordprocessingml.document"
              extensions=".docx" icon="word_mime"/>
    <mimetype name="application/x-msexcel" extensions=".xls" icon="xls_mime"/>
    <mimetype name="audio/wav" extensions=".wav" icon="sound_mime"/>
    <mimetype name="audio/x-ms-wma" extensions=".wma" icon="sound_mime"/>
    <mimetype name="image/bmp" extensions=".bmp" icon="image_mime"/>
    <mimetype name="image/gif" extensions=".gif" icon="image_mime"/>
    <mimetype name="image/jpeg" extensions=".jpg" icon="image_mime"/>
    <mimetype name="image/pjpeg" extensions=".jpg" icon="image_mime"/>
    <mimetype name="image/tiff" extensions=".tiff|.tif" icon="image_mime"/>
    <mimetype name="image/vnd.ms-modi" extensions=".mdi" icon="document"/>
    <mimetype name="image/png" extensions=".png" icon="image_mime"/>
    <mimetype name="image/x-png" extensions=".png" icon="image_mime"/>
    <mimetype name="text/html" extensions=".html|.htm" icon="html_mime"/>
    <mimetype name="text/plain" extensions=".txt" icon="text_mime"/>
    <mimetype name="text/richtext" extensions=".rtx" icon="text_mime"/>
    <mimetype name="text/xml" extensions=".xml" icon="html_mime"/>
    <mimetype name="video/mpeg" extensions=".mpg|.mpeg" icon="video_mime"/>
    <mimetype name="video/quicktime" extensions=".mov" icon="video_mime"/>
    <mimetype name="video/x-msvideo" extensions=".avi" icon="video_mime"/>
  </mimetypemapping>

