<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <RowSet
    id="ABCompanyMergeDetailsRowSet"
    mode="default">
    <Require
      name="companies"
      type="ABCompany[]"/>
    <Require
      name="mergedCompany"
      type="ABCompany"/>
    <Require
      name="nameOwner"
      type="gw.api.name.NameOwner"/>
    <Row
      visible="nameOwner.isVisible(gw.api.name.NameOwnerFieldId.NP_NAME)">
      <TextCell
        available="nameOwner.isAvailable(gw.api.name.NameOwnerFieldId.NP_NAME)"
        id="nameLabel"
        value="nameOwner.ContactNameLabel"/>
      <CellIterator
        elementName="aContact"
        id="name"
        value="companies"
        valueType="entity.ABCompany[]">
        <TextCell
          available="nameOwner.isAvailable(gw.api.name.NameOwnerFieldId.NP_NAME)"
          editable="aContact == mergedCompany &amp;&amp; nameOwner.isEditable(gw.api.name.NameOwnerFieldId.NP_NAME)"
          id="name"
          required="nameOwner.isRequired(gw.api.name.NameOwnerFieldId.NP_NAME)"
          value="aContact.Name"/>
      </CellIterator>
    </Row>
  </RowSet>
</PCF>